/* pages/start/start.wxss */
/**start.wxss**/
page,.container{
  background-color: #fff;
  width: 100%;
  height: 100%;
  display: block;
}
.tab-image{
  width: 100%;
  height: 75vh;
  position: relative;
}
.bgImages{
  width:100%;
  display:block;
  height:75vh;
}
.logo{
  width:100%;
  height:25vh;
  color:#333;
  background-color:#ffffff;
  text-align:center;
}
.title{
  font-size:40rpx;
  margin-top:45px;
  background-color:#b5272d;
  display:inline-block;
  color:#fff;
  padding:5px 8px;
  border-radius:3px;
}
.subtitle{
  font-size:28rpx;
  color:#666;
  margin-top: 10px;
}
.gohome{
  font-size:24rpx;
  background-color:rgba(0,0,0,.5);
  color:#fff;
  text-align:center;
  float:right;
  position:absolute;
  right:10px;
  bottom:10px;
  border-radius:20px;
  padding:3px 8px;
}