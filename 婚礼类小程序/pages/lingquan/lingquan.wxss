/* pages/lingquan/lingquan.wxss */
page{
  background: #f7f7f7;

}
.nimen_ge{ 
 width: 100%; 
}
.nimen_ge .ul{
  width: 94%;
  margin: auto;
  padding: 20px 0
}
.nimen_ge .li{
  background:url(data:image/png;base64,);
  background-size:100%;
  height: 310rpx;
  background-repeat: no-repeat;
  position: relative;
}
.nimen_ge .li .zit{
  padding: 20rpx 30rpx;
  font-weight: 600;
}

.nimen_ge .li .qiang_ge{
  font-size: 60rpx;
  padding: 0 40rpx;
  color: #d8555b;
  overflow: hidden;
}
.nimen_ge .li .qiang_ge view{
  font-size: 30rpx;
  float: left;
  margin-top: 30rpx;

}

.nimen_ge .li .qiang_ge text{
  float: left;
}

.nimen_ge .li .qiang_ge view:last-child{
  padding-left:100rpx;
}

.nimen_ge .li button{
  position: absolute;
  top: 100rpx;
  right: 20px;
  width: 110px;
  font-size:30rpx;
  background: #ec6585;
  border-radius: 100rpx;
  color: #fff;
  padding: 8px 0;

}

.nimen_ge .li .buttom{
  padding: 0 30rpx;
  padding-top: 40rpx;
  color: #999;

}