}
.hou_biaot text::after{
  content: "";
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 4rpx;
  height: 32rpx;
  background: #c49669;
}

.hou_biaot image{
  width: 50rpx;
  height: 50rpx;
  display: block;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto
}

