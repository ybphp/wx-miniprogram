.index_list .text  .shum{ 
overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;
  font-size: 26rpx;
padding: 10rpx 0;
color: #a8a8a8;

}
.index_list .text  .dianz{ 
  overflow: hidden;
  /* float: right; */
