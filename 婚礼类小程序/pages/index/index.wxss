}
.huny_daih swiper{
 height:380rpx;
   

}
.huny_daih swiper swiper-item{
  /* background: #eee */
}

.huny_daih swiper .kuang{
  width: 95%;
  height:300rpx;
  background: #fff;
  box-shadow: 0 0 15rpx #eee;
  margin:2.5%; 
  border-radius: 10rpx;
  
}

.huny_daih swiper .kuang .li{
  width: 50%;
  height:150rpx;
  border-right: solid 1rpx #eee;
  position: relative;
  background: #fff;
  margin-left: -1px;
  float: left; 
}
.huny_daih swiper .kuang .li:nth-child(2n){
  float: right;
  border-right:solid 1rpx #eee; 
}
.huny_daih swiper .kuang .li:nth-child(1){
  border-bottom: solid 1rpx #eee;
  border-top-left-radius: 10rpx;
}
.huny_daih swiper .kuang .li:nth-child(2){
  border-bottom: solid 1rpx #eee;
  border-top-right-radius: 10rpx;

}
.huny_daih swiper .kuang .li:nth-child(3){
  border-bottom-left-radius: 10rpx;

}
.huny_daih swiper .kuang .li:nth-child(4){
  border-bottom-right-radius: 10rpx;

}
.huny_daih swiper .kuang .li .fd{
  position: absolute;
  top:  20rpx;
  left: 10rpx;
  line-height:45rpx;
}
.huny_daih swiper .kuang .li image{
  width: 100rpx;
  height: 100rpx;
  float: right;
  margin-right: 10rpx;
  margin-top: 20rpx;
}
.huny_daih swiper .kuang .li .fd .h1{
  color: #ac6716;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;

}
.huny_daih swiper .kuang .li .fd .p{
  font-size: 24rpx;
  width: 230rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #999999;

}



.index_bt{
