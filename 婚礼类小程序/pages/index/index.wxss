/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}


/* 我的开始了 */

.namak_h{
  padding: 0 8rpx;
} 
.namak_h .nima_dah{
  border-left: solid 4rpx #ff8f8e;
  font-size:48rpx;
  line-height: 40rpx;
  padding-left: 10rpx;
  color: #ff8f8e;
}

/* banner 开始了 */
.banner_sy{
  position: relative;
  z-index: 1; 
}
.banner_sy swiper{
height: 480rpx; 
  position: relative;
  z-index: 2;
  margin: 20rpx; 
border-radius: 10rpx;

}
.banner_sy swiper swiper-item{
  height: 480rpx!important; 
    border-radius: 10rpx;
}
.banner_sy swiper-item image{
  width: 100%; 
  height: 420rpx; 
  object-fit: cover;
  border-radius: 10rpx;

}
 
.banner_sy .text{
  position: absolute;
  left:20rpx;
  width: 350rpx;
  height: 110rpx;
  bottom:6rpx;
  padding: 20rpx;
  padding-top: 10rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 5px #eee; 
}

.banner_sy .text .i_henx{
  width: 1.5em;
  height: 4rpx;
  margin: 10rpx 0;
  margin-top: 16rpx;
  background: #ac6716;
}
.banner_sy .text .jianj{
  color: #444;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.banner_sy .text .text_xia{
  font-size: 28rpx;
  color: #999;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;

}


/*用来包裹所有的小圆点  */
.dots{  
  width: 90%;
  height: 15rpx; 
  display: flex;
justify-content : flex-end;
  position: absolute;
  left: 0;
  bottom: 20rpx;
  padding: 0 5%;
  text-align: center;
}  
/*未选中时的小圆点样式 */
.dot{  
  width: 12rpx;  
  height: 12rpx;   
  border-radius: 50%;
  margin-right: 10rpx;
  background-color: #eee;
  display: inline-block;
}  
/*选中以后的小圆点样式  */
.active{  
  width: 30rpx;   
  border-radius: 8rpx;
  height: 12rpx;
  background-color:#ac6716;
}   


/* banner 开始了结束 */



