/* pages/list/list.wxss */

.list_daohang{
  height: 100rpx;
  line-height: 100rpx;
}

.tab {
  /* float: left; */
  /* width: 33.3333%; */
  text-align: center;
  padding:0 20rpx;
  height: 100%;
  display: inline-block;
  white-space: nowrap;  
  color: #6b6b6b;
}
 
.topTabSwiper{
  /* border-top: 1px solid #ccc; */
  /* border-bottom: 1px solid #ccc; */
  zoom: 1;
  height: 100%;
  width: 85%;
  white-space:nowrap;  
  overflow: hidden;
  overflow-x: scroll;
  float: left;
}
 
.topTabSwiper:after{
  content: "";
  clear: both;
  display: block;
}
 
.tabBorer {
  /* border-bottom: 1px solid #f00; */
  color: #cfa972;
  height: 100rpx;
  position: relative;
}
.tabBorer::after{
  content: "";
  background: #cfa972;
  display: block;
  height: 4rpx;
  border-radius:2rpx;
  width:1.5em;
  position: absolute;
  bottom:0;
  left: 0;
  right: 0;
  margin: auto;
}

.tabBorer::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.swiper {
  width: 100%;
}
 
/* .swiper_con {
  text-align: center;
  width: 100%;
  height: 100%;
  padding: 80rpx 0;
} */
.list_daohang .sous_k{
    width: 15%;
    float: right;
    height: 100%;
    position: relative;
    
}

.list_daohang .sous_k::after{
  position: absolute;
  content: "";
  display: block;
  background-image: linear-gradient(to right, rgba(255,255,255,.0),#fff);
  width: 40rpx;
  height: 100%;
  left: -40rpx;
}
.list_daohang .sous_k::before{
  width: 2rpx;
  height: 40rpx;
  background: #eee;
  content: "";
  position: absolute;
  margin:auto;
  top: 0;
  bottom: 0; 
}


.list_daohang .sous_k image{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}

/*  */

.index_list{
  padding: 2.5%;  
  /* background: #f00; */
  overflow: hidden;
}

.index_list>view{
  border-radius: 15rpx;
  overflow: hidden;
  background-image:url("data:image/jpeg;base64,");
   background-repeat: no-repeat;
  background-position:right 50rpx bottom;
    background-size: 180rpx;
  margin-bottom: 20rpx;
    box-shadow: 0 0 10rpx #eee;
}
.index_list>view image{
  width: 100%;
  height: 200px;
  object-fit:cover;
  display: block;
}

.index_list .text{
  margin: 10rpx;
}
.index_list .text .bt{
  color: #4d4d4d;
  overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;

}
.index_list .text .bt .span{
  display: inline-block;
  background: #ff8f8e;
  color: #fff;
  padding:8rpx 10rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  border-radius: 5rpx;
  margin-right: 10rpx;
}
.index_list .text  .shum{ 
overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis;
  font-size: 26rpx;
padding: 10rpx 0;
color: #a8a8a8;

}
.index_list .text  .dianz{ 
  overflow: hidden;
  /* float: right; */
  text-align: right;
}

.index_list .text  .dianz view{
  /* float: right */
  display: inline-block;
  background-image: url("data:image/png;base64,") ;
  color: #a8a8a8;
  background-repeat: no-repeat;
  background-position:left center;
  background-size: 32rpx;
  padding: 0 20rpx 0 35rpx;
  font-size: 24rpx;

}
.index_list .text  .dianz view:nth-child(2){ 
 background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position:left center;
    background-size: 32rpx;
}

.swiper{
  height: 3000rpx;
  overflow: hidden;
  
}


/**index.wxss**/
/* 外面的按钮 */
.btn{
  width: 80px;
  height: 35px;
  background: #44b549;
  line-height: 35px;
  text-align: center;
  color: #fff;
  font-size: 15px;
  margin:20px auto;
  border-radius: 100px;
}
 
/* 遮罩层 */
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.5;
}
  
/* 弹出层 */
.modalDlg{
    width:100%;
    position: fixed;
    top: 0px;
    left: 0;
    right: 0;
    z-index: 9999;
    margin: 0 auto;
    background-color: #fff;
    border-radius:0;
    display: flex;
    flex-direction: column;
    align-items: center;
  box-shadow:0 0 10rpx #666;

}
.modalDlg input{
  width: 80%;
  padding: 0 2%;
  margin:20rpx 8%;
  float: left; 
  background: #eee;
  height: 80rpx;
  border-radius: 10rpx;
}
.modalDlg .ok{
  width: 20%;

}
/* 弹出层里面的图片 */
.modalDlg image{
  width: 120px;
  height: 120px;
  margin-top: 30px;
}
 
/* 弹出层里面的按钮 */
.modalDlg .ok{
  width: 200rpx;
  height: 65rpx;
  background: #44b549;
  line-height: 65rpx;
  text-align: center;
  color: #fff;
  font-size: 15px;
  margin: 20rpx auto;
  border-radius: 100px;
  margin-top: 0;
}
 
/* 弹出层里面的文字 */
.modalDlg .text{
  text-align: justify;
  font-size: 14px;
  color: #666;
  width: 180px;
  margin-top: 10px;
}

swiper-item {
  overflow: scroll;
}