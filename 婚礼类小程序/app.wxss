/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
 
page::after{
    content: '';
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    background-color: #eee;
    z-index: 9999;
}

.rpx20{
  height: 20rpx;background: #f5f5f5;
}


@import 'style/weui.wxss'