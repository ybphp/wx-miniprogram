/* pages/myCoupon/myCoupon.wxss */
@import '/assets/css/weui.wxss';
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__panel{
  padding-top:47px;
}
.weui-navbar{
  height:80rpx;
  border-bottom: none;
  margin-top:12rpx;
  background-color: #fff;
  font-size: 26rpx;
  color:#282828;
  display: flex;
  align-items: center;
}
.weui-navbar__item{
  border-top:none;
  border-bottom:2rpx solid #EFEFF4;
  padding:0;
  height:80rpx;
  line-height:80rpx;
}
.weui-navbar__item.weui-bar__item_on{
  color:#F44225;
}
.weui-navbar__slider{
  background-color: #F44225;
}
/* 列表 */
.list{
  padding:0 24rpx;

}
.listItem{
  display: flex;
  margin-top: 40rpx;
}
.listItem .bgImg{
 width:170rpx;
 height:150rpx;
 background-image:url(data:image/png;base64,);
 color:#fff;
 background-size: contain;
 display: flex;
 justify-content: center;
 align-items: center;
 font-size: 24rpx;
}
.listItem .bgImg view{
  padding-left:20rpx;
}
.listItem .bgImg text{
  font-size: 48rpx;
}

.listItem .bgImg2{
 background-image:url(data:image/png;base64,);
}
.listItem .cont{
  background-color: #fff;
  flex: 1;
  height:102rpx;
  display: flex;
  flex-direction: column;
  padding:24rpx;
}
.listItem .cont view:nth-child(1){
font-size: 24rpx;
color: #282828;
flex: 1;
font-weight:500
} 
.listItem .cont view:nth-child(2){
font-size: 20rpx;
color: #C5C5C5;
display: flex;
flex: 1;
align-items: flex-end
} 
.listItem .cont view:nth-child(2) text{
  flex: 1
}
.listItem .cont button{
  padding:0;
  border-radius: 18rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width:100rpx;
  height:36rpx;
  background-color:#fff ;
  
}
.cRed{
color: #F44225;
  border:2rpx solid #F44225;
}
.cGray{
  color: #C5C5C5;
  border:2rpx solid #C5C5C5;
}

/* 没有数据显示 */
.noData{
  text-align: center;
}
.noData image{
  width:98rpx;
  height:98rpx;
  margin-top:360rpx;
  margin-bottom:24rpx;
}
.allData{
padding:24rpx;
text-align: center;
color:#C5C5C5;
font-size:24rpx;
}