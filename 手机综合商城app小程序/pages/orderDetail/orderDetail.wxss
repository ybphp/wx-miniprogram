/* pages/orderDetail/orderDetail.wxss */
.restTime{
  height:152rpx;
  background-image: url(data:image/png;base64,);
  background-size:contain;
  color:#fff;
  font-size:26rpx;
}
.restTime view{
  padding:30rpx 80rpx 0rpx;
}
.restTime view:nth-child(2){
  padding-top: 20rpx;
}
.logistics,.addr{
  font-size:24rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  padding:24rpx 21rpx;
}
.logistics{
  color:#3FA01D;
  border-bottom: 2rpx solid #EFEFF4
}
.logistics view{
  flex:1
}
.logistics image:nth-child(1){
  width:32rpx;
  height:28rpx;
  margin-right: 24rpx;
  vertical-align: middle
}
.logistics image:nth-child(2){
  width:16rpx;
  height:26rpx;
}
.oUser{
  flex:1
}
.addr image{
  width:22rpx;
  height:32rpx;
   margin-right: 34rpx;
}
.addr .addrs{
  color:#C5C5C5;
  padding-top:10rpx;
}
.addr .orderUser{
  display: flex
}
.addr .orderUser text{
  flex:1
}
.addr .orderUser text:nth-child(2){
 text-align: right
}

/* list */
.list{
  background-color: #fff;
  margin-bottom:24rpx;
  margin-top:12rpx;
}
.list .listTitle{
  font-size: 24rpx;
  display: flex;
  align-items: center;
  height:80rpx;
  padding:0 24rpx;
  border-bottom: 2rpx solid #EFEFF4;
}
.list .listTitle text{
  flex:1;
}
.list .listTitle .num{
  color:#C5C5C5
}
.list .listItem{
  height:189rpx;
  display: flex;
  align-items: center;
  padding:0 24rpx;
}
.listItem .detail{
 height:100%;
 flex:1;
 border-bottom: 2rpx solid #efeff4;
 display: flex;
 align-items: center;
}
.listItem:last-child .detail{
  border:none;
}
.detail_r{
  flex:1;
  height:140rpx;
  font-size:24rpx;
  color:#282828;
}
.detail_r text{
  display: block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.detail_r .detail_rT text:nth-child(1){
  padding:14rpx 0;
}
.detail_r text:nth-child(2){
 font-size: 20rpx;
 color:#c5c5c5;
}
.detail_rT{
  margin-bottom: 10rpx;
}
.detail_rB{
  display: flex;
  /* width:478rpx; */
  align-items: center
}
.detail_rB text{
  flex:2
}
.detail_rB .stepper{
  flex: 1;
  text-align: right
}
.listItem image{
  width:140rpx;
  height:140rpx;
  border-radius: 8rpx;
  margin-right:24rpx;
}
.orderTotal{
  height:230rpx;
  padding:0 24rpx;
  font-size: 20rpx;
  color:#C5C5C5;
  border-top:2rpx solid #efeff4
}
.orderTotal view{
  margin-bottom:12rpx;
}
.orderTotal .allmoney{
  font-size: 22rpx;
  color:#282828;
  margin:22rpx 0;
}
/* 底部固定*/
.bottomCont{
  height:98rpx;
  width:100%;
  position: fixed;
  bottom:0;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: flex-end
}
.bottomCont button{
  width:160rpx;
  height:60rpx;
  line-height:60rpx;
  border-radius: 30rpx;
  font-size:26rpx;
  text-align: center;
  margin:0 28rpx 0 0 ;
  background-color: #fff;
  padding:0;
}
.bottomCont button:nth-child(1){
 border:2rpx solid #C5C5C5;
 color:#C5C5C5
}
.bottomCont button:nth-child(2){
  border:2rpx solid #F44225;
  color:#F44225
}
